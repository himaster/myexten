{
	"manifest_version": 2,

	"name": "PKW Project Selector",
	"description": "Select one of the PKW sites you want to go to.",
	"version": "1.1.5",

	"minimum_chrome_version": "22",

	"permissions" : [
		"tabs",
		"storage",
		"http://aws.pkwteile.de/ce/*"
	],

	"update_url" : "https://dl.dropbox.com/u/9059467/chrome-ext/pkw-project-selector/updates.xml",

	"browser_action": {
		"default_icon": "favicon.ico",
		"default_popup": "project.html"
	}
}
